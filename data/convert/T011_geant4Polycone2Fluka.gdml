<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="sphi" value="0"/>
		<constant name="dphi" value="1.5*pi"/>
		<constant name="prmin1" value="7"/>
		<constant name="prmax1" value="9"/>
		<constant name="z1" value="-10"/>
		<constant name="prmin2" value="5"/>
		<constant name="prmax2" value="9"/>
		<constant name="z2" value="0"/>
		<constant name="prmin3" value="4"/>
		<constant name="prmax3" value="5"/>
		<constant name="z3" value="10"/>
	</define>
	<materials>
		<isotope name="G4_H_1" Z="1" N="1">
			<atom value="1.007825"/>
		</isotope>
		<isotope name="G4_H_2" Z="1" N="2">
			<atom value="2.014102"/>
		</isotope>
		<element name="G4_H" formula="H">
			<fraction ref="G4_H_1" n="0.999885"/>
			<fraction ref="G4_H_2" n="0.000115"/>
		</element>
		<material name="G4_Galactic" state="gas">
			<D value="1e-25"/>
			<fraction ref="G4_H" n="1.0"/>
		</material>
		<isotope name="G4_Fe_54" Z="26" N="54">
			<atom value="53.9396"/>
		</isotope>
		<isotope name="G4_Fe_56" Z="26" N="56">
			<atom value="55.9349"/>
		</isotope>
		<isotope name="G4_Fe_57" Z="26" N="57">
			<atom value="56.9354"/>
		</isotope>
		<isotope name="G4_Fe_58" Z="26" N="58">
			<atom value="57.9333"/>
		</isotope>
		<element name="G4_Fe" formula="Fe">
			<fraction ref="G4_Fe_54" n="0.05845"/>
			<fraction ref="G4_Fe_56" n="0.91754"/>
			<fraction ref="G4_Fe_57" n="0.02119"/>
			<fraction ref="G4_Fe_58" n="0.00282"/>
		</element>
		<material name="Material_G4_Fe" state="solid">
			<D value="7.874"/>
			<fraction ref="G4_Fe" n="1.0"/>
		</material>
	</materials>
	<solids>
		<box name="ws" x="100" y="100" z="100" lunit="mm"/>
		<polycone name="ps" startphi="0" deltaphi="1.5*pi" lunit="mm" aunit="rad">
			<zplane rmin="7" rmax="9" z="-10"/>
			<zplane rmin="5" rmax="9" z="0"/>
			<zplane rmin="4" rmax="5" z="10"/>
		</polycone>
	</solids>
	<structure>
		<volume name="pl">
			<materialref ref="Material_G4_Fe"/>
			<solidref ref="ps"/>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="p_pv1">
				<volumeref ref="pl"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>
